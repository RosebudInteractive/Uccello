{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["./"],
    "exclude": ["./node_modules/", "./docs/", "./uses/", "./jsdoc", "./lib"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "private" : true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemName"            : "Uccello",
    "footer"                : "Copyright (c)",
    "copyright"             : "Rosebud",
    "theme"                 : "cerulean",
    "includeDate"           : true,
    "navType"               : "vertical",
    "linenums"              : true,
    "collapseSymbols"       : true,
    "inverseNav"            : false,
    "outputSourceFiles"     : true,
    "outputSourcePath"      : true,
    "syntaxTheme"           : "default",
    "sort"                  : true
  }
}